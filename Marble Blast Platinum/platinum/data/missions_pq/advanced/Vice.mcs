$VV::MegaManFadeInOut = 500; // 0.5 seconds of transition.
$VV::MegaManTimer = 1500; // The 1.5 seconds matan wanted.
$VV::MegaManHideAgain = $VV::MegaManFadeInOut + $VV::MegaManTimer;

function MegaManEmulationTrigger::onEnterTrigger(%this, %trigger, %obj) {
   // and start the simulation!
   //
   // TODO: only make it happen once per "round"
   //   as in you need to hit checkpoint for it to allow the megaman loop to 
   //   happen again, or restarting the level.

   // The first platform stays up until you hit it. The onCollision event fires the loop.
   // This is for the pissy players who are retarded at this game and have no idea what
   // the hell goes on.
   // AKA 99% of the community :^)
   %current = %trigger.startPlatform;
   %current.respondToCollision = true; // only the first one gets this field.
   %current.hasCollided = false;

   // Unhide the first one and wait until onCollision like stated above.
   %current.hide(false);
   %current.startFade($VV::MegaManFadeInOut, 0, 0);
}

function MegaManPlatform::onCollision(%this, %obj, %col) {
	if (!isObject(%obj.next)) {
		//Don't despawn the platform if it's the last one (ie has no next)
		// so you can wait for the next platforms to be visible
		cancel(%obj.fadeTimer);
		cancel(%obj.hideAgainTimer);
		cancel($vv_mmLoop[%obj]);
	} else if (!%obj.respondToCollision || %obj.hasCollided) {
		return;
	}
	%obj.hasCollided = true;

   // do the fade shit and hide this one. The loop takes care of the rest.
   %obj.fadeTimer = %obj.schedule($VV::MegaManTimer * 2, "startFade", $VV::MegaManFadeInOut, 0, 1);
   %obj.hideAgainTimer = %obj.schedule($VV::MegaManHideAgain * 2, "hide", true);

   // now we can start the loopy loop
   $vv_mmLoop[%obj] = schedule($VV::MegaManTimer, 0, vv_megaManLoop, %obj, %obj.next);
}

function MegaManPlatform::onMissionReset(%this, %obj) {
   // cancel schedules.
   cancel(%obj.fadeTimer);
   cancel(%obj.hideAgainTimer);

   if ($vv_mmLoop[%obj] !$= "")
      cancel($vv_mmLoop[%obj]);

   // Reset to allow the mega man loop to go again.
   if (%obj.hasCollided)
      %obj.hasCollided = false;

   // rehide the mega man platform.
   %obj.hide(true);
   %obj.startFade(0,0,1);   
}

function vv_megaManLoop(%start, %current) {
   cancel($vv_mmLoop[%start]);
   if (!isObject(%current))
      return;

   // show the current platform.
   // hide it 2X later. This allows for 1st platform shows, second platform shows $VV::MegaManTimer milliseconds seconds later,
   // then $VV::MegaManTimer milliseconds later that hides and the next one shows. Fading is done to transition them, so the actual
   // collision object stays up for longer ($VV::MegaManFadeInOut fade in, $VV::MegaManTimer idle, $VV::MegaManFadeInOut fade out).
   %current.hide(false);
   %current.startFade($VV::MegaManFadeInOut, 0, 0);
   %current.fadeTimer = %current.schedule($VV::MegaManTimer * 2, "startFade", $VV::MegaManFadeInOut, 0, 1);
   %current.hideAgainTimer = %current.schedule($VV::MegaManHideAgain * 2, "hide", true);


   $vv_mmLoop[%start] = schedule($VV::MegaManTimer, 0, vv_megaManLoop, %start, %current.next);
}

