function VVNameDlg::onWake(%this) {
	%i = 0; 
	while (isFile("platinum/data/state/vvRun" @ %i @ ".txt")) {
		%i ++;
	}

	VVND_Name.setValue("Run" @ %i);
	%this.checkName();
}

function VVNameDlg::dosave(%this) {
	%filename = "platinum/data/state/vv" @ VVND_Name.getValue() @ ".txt";
	if (isFile(%filename)) {
		MessageBoxOk("Error", "That run save already exists.");
		return;
	}

	vv_saveState(%filename);
	Canvas.popDialog(%this);
	EG_Next.setActive(1);
}

function VVNameDlg::discard(%this) {
	Canvas.popDialog(%this);

	//This would break Versa because you'd try to load without a save
	EG_Next.setActive(0);
}

function VVNameDlg::checkName(%this) {
	VVND_Name.setValue(alphaNum(VVND_Name.getValue()));
	VVND_Save.setActive(VVND_Name.getValue() !$= "");
}


