function vv_saveState(%path) {
	%state = "";
	%count = getWordCount($VV::List);
	for (%i = 0; %i < %count; %i ++) {
		%obj = getWord($VV::List, %i);
		%data = "";
		if (isObject(%obj))
			%data = %obj.getDataBlock().saveVVState(%obj);
		//echo(%obj.getName() SPC %data);
		%state = %state @ %data @ ";";
	}

	%state = %state @ $Time::CurrentTime @ ";";
	%state = %state @ LocalClientConnection.player.powerUpData.getName() @ ";";
	%state = %state @ lb() @ ";";

	//echo("VV State:");
	//echo(%state);
	%state = strEnc(%state, 2);
	fwrite(%path, %state);

	//In case we press "next"
	$VV::LoadFile = %path;
}

function serverCbOnOutOfBounds() {
   // Find all Mega Man Platforms and restart them when we go out of bounds.
   %count = FPGroup.getCount();
   for (%i = 0; %i < %count; %i++) {
      %fp = FPGroup.getObject(%i);
      %d = %fp.getDataBlock();
      if (%d.className $= "MegaManPlatform")
         %d.onMissionReset(%fp);
   }
}

package ViceEndNext {
	function EndGameDlg::onWake(%this) {
		Parent::onWake(%this);
		if (!MissionInfo.vice) {
			deactivatePackage(ViceEndNext);
			return;
		}
		if ($pref::highscores[$Server::MissionFile, 0] $= "") {
			if ($VVNextOldCommand $= "") {
				$VVNextOldCommand = EG_Next.command;
				EG_Next.command = "Canvas.popDialog(EndGameDlg); vv_next();";
			}
			$VVReplace = 1;
		} 
		if ($VVReplace) {
			EG_Replay.setActive(0);
			EG_Continue.setActive(0);
		}
	}
	function clientCmdGameEnd() {
		Parent::clientCmdGameEnd();
		if (!MissionInfo.vice) {
			deactivatePackage(ViceEndNext);
			return;
		}

		if ($pref::highscores[$Server::MissionFile, 0] $= "") {
			vv_saveState("platinum/data/state/vvFirstRun.txt");
		} else {
			Canvas.pushDialog(VVNameDlg);
			VVND_Name.makeFirstResponder(true);
		}
	}
};

function MissionStartup() {
	activatePackage(ViceEndNext);
}

function vv_next() {
	deactivatePackage(ViceEndNext);
	EG_Replay.setActive(1);
	EG_Continue.setActive(1);
	if ($VVNextOldCommand !$= "") {
		EG_Next.command = $VVNextOldCommand;
		$VVNextOldCommand = "";
		$VVReplace = 0;
	} 

	//Sneaky!
	PlayMissionGui.switchMission(PlayMissionGui.selectedIndex + 1);
	loadMission(expandFilename("./Versa.mcs"));
	$VV::LoadFile = "platinum/data/state/vvFirstRun.txt";

	//Update this because otherwise it looks stupid
	PlayMissionGui.showMissionList();
}

