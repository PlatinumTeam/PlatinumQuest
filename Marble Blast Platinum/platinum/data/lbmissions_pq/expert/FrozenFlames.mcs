//Mission information for the level select. Generated from the MissionInfo object except with extra goodies.
function LB_PQ_FrozenFlames_GetMissionInfo() {
	return
new ScriptObject() {
		name = "Frozen Flames";
		type = "Expert";
		level = "3";
		desc = "The expert levels are starting to heat up...";
		startHelpText = "Head straight after launching from the cannon.";
		artist = "Threefolder";
		music = "Waves in the Dark.ogg";
		game = "PlatinumQuest";
		platinumTime = "100000";
		ultimateTime = "68000";
		awesomeTime = "24000";
		generalHint = "All of the challenges in this level revolve around the Fire Blast -- it\'s up to you to determine how to use it.";
		ultimateHint = "You need to find an optimal path through all of the Ice Shard mazes. Keep on the lookout for shortcuts only possible with the Fire Blast ability.";
		awesomeHint = "Start by doing this level backwards, getting what normally is the first Gem as the last one. Don't use the cannon at all for this challenge!";
		CustomRadarRule = $Radar::Flags::Gems | $Radar::Flags::EndPad | $Radar::Flags::Checkpoints | $Radar::Flags::Powerups;
		easterEgg = "0";
		gems = "3";
		interior0 = $usermods @ "/data/lbinteriors_pq/expert/frozenFlames.dif";
		interior1 = $usermods @ "/data/lbinteriors_pq/expert/frozenFlames_mp.dif";
		interior2 = $usermods @ "/data/lbinteriors_pq/expert/frozenFlames_mp2.dif";
		interiors = "3";
		maxScore = "3";
		requirements = "Finish Trial by Fire and Advanced Techniques.";
		trivia = "It\'s not unlikely that this level is what prevented the Fireball powerup from being cut.";
		unlockFunc = "unlock_LB_FrozenFlames";
};
}
//--- INFO END ---
//--- UNLOCK BEGIN ---
function unlock_LB_FrozenFlames(%mission) {
   return Unlock::hasBeatMissionPar(getMissionInfo($usermods @ "/data/lbmissions_pq/beginner/TrialByFire.mcs")) &&
   	Unlock::hasBeatMissionPar(getMissionInfo($usermods @ "/data/lbmissions_pq/beginner/AdvancedTechniques.mcs"));
}
//--- UNLOCK END ---
//Don't continue loading if this just wants the mission info
if ($loadingMissionInfo) return;
