//--- UNLOCK BEGIN ---
function unlock_LB_WhiteNoise(%mission) {
	return (Unlock::getMissionCompletion(getMissionInfo($usermods @ "/data/lbmissions_pq/intermediate/AboveAndBelow.mcs")) & $Completion::Ultimate) &&
		(Unlock::getMissionCompletion(getMissionInfo($usermods @ "/data/lbmissions_pq/expert/Orthogonality.mcs")) & $Completion::Ultimate);
}
//--- UNLOCK END ---
//Don't continue loading if this just wants the mission info
if ($loadingMissionInfo) return;
//--- CLIENT SCRIPTS BEGIN ---
//Put any scripts that will be loaded on all clients (in MP / SP) here.
// Note: these will be loaded by dedicated servers too, so be sure to test for
// $Server::Dedicated before creating any GUI.

function clientCbOnMissionReset() {
	if ($Game::Menu)
		return;
	activatePackage(WhiteNoise);
}

function clientCmdSMBTrigger(%impulse, %upwards) {
	$SMBTrigger = %impulse;
	$SMBUpwards = %upwards;
	echo("SMBTrigger set to " SPC %impulse SPC %upwards);
}

package WhiteNoise {
	function Marble::onJump(%this) {
		Parent::onJump(%this);

		if (MissionInfo.whiteNoise) {
			if ($SMBTrigger != 0) {
				%this.applyImpulse("0 0 0", "0 0" SPC $SMBTrigger);
			}
			if ($SMBUpwards != 0) {
				%v = %this.getVelocity();
				%this.setVelocity(setWord(%v, 2,  max(getWord(%v, 2), $SMBUpwards)));
			}
		}
	}

	function SceneObject::getSurfaceVelocity(%this, %marble, %contact, %dist) {
		if (strpos(%this.getFieldValue("interiorFile"), "Buzzsaw.dif") != -1) {
			//Nope
			commandToServer('Buzzsawd', %marble.getSyncId());
		} else {
			return Parent::getSurfaceVelocity(%this, %marble, %contact, %dist);
		}
	}
};

new MaterialProperty(PQWallStickMaterial) {
	friction = 1;
	restitution = 0;
};
addMaterialMapping("stickconcrete", PQWallStickMaterial);

