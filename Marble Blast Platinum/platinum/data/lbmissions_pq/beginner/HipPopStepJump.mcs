function HPSJTrigger::onEnterTrigger(%this, %trigger, %obj)
{
  if ($Game::ResetTime !$= $HPSJ::ResetTime)
  {
    $HPSJ::ResetTime = $Game::ResetTime;
    InitLoopMahMP();
  }
}

function InitLoopMahMP()
{
  if ($Game::ResetTime !$= $HPSJ::ResetTime)
    return;
  schedule(4351, 0, "LoopMahMP", 1, 1351);
  schedule(4326, 0, "LoopMahMP", 2, 1326);
  schedule(4451, 0, "LoopMahMP", 3, 1451);
  schedule(3651, 0, "LoopMahMP", 4, 651);
  schedule(4251, 0, "LoopMahMP", 5, 1251);
  schedule(3426, 0, "LoopMahMP", 6, 426);
  schedule(4051, 0, "LoopMahMP", 7, 1051);
  schedule(4301, 0, "LoopMahMP", 8, 1301);
  schedule(3801, 0, "LoopMahMP", 9, 801);
  schedule(3726, 0, "LoopMahMP", 10, 726);
  schedule(3676, 0, "LoopMahMP", 11, 676);
  schedule(3701, 0, "LoopMahMP", 12, 701);
}

function LoopMahMP(%mp, %time)
{
  if (!$Game::Running)
    return;

  if ($Game::ResetTime !$= $HPSJ::ResetTime)
    return;
  %obj = "LoopMP" @ %mp;
  %obj.setPathPosition(3000);
  %obj.setTargetPosition(100000);
  schedule(%time, 0, "LoopMahMP", %mp, %time);
}

