//--- CLIENT SCRIPTS BEGIN ---
//Put any scripts that will be loaded on all clients (in MP / SP) here.
// Note: these will be loaded by dedicated servers too, so be sure to test for
// $Server::Dedicated before creating any GUI.

package BubbleOverride {
	function UseBubble(%active) {
		Parent::UseBubble(%active);
		if (%active) {
			$IntoTheDeepUsedBubble = true;
		}
	}
};

function clientCbOnMissionReset() {
	activatePackage(BubbleOverride);
	$IntoTheDeepUsedBubble = false;
}

function clientCbCheckEndgameAchievements() {
	// Activate achievement if no SS was used, bubble wasn't used, and
	// timetravel wasn't picked up.
	%superspeed = $Client::UsedPowerup[2];
	if (!%superspeed && !$IntoTheDeepUsedBubble && PlayGui.bonusTime == 0) {
		activateAchievement($Achievement::Category::Level, 0);
	}

	$IntoTheDeepUsedBubble = false;
}

//--- CLIENT SCRIPTS END ---
//Don't continue loading if this is a client (non-server)
if (!$Server::Hosting || $Server::_Dedicated) return;
