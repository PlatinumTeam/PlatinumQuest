//Mission information for the level select. Generated from the MissionInfo object except with extra goodies.
function LB_PQ_IntoTheDeep_GetMissionInfo() {
	return
new ScriptObject() {
		name = "Into the Deep";
		type = "Beginner";
		level = "1";
		desc = "Hit the cool waters, collect the treasure and learn about the Bubble Powerup.";
		startHelpText = "Got your goggles? Time to go diving!!";
		artist = "Matan";
		music = "Tranquility.ogg";
		game = "PlatinumQuest";
		platinumTime = "55000";
		ultimateTime = "34000";
		awesomeTime = "14000";
		generalHint = "Water affects your marble by slowing it down, but also has slightly decreased gravity, meaning your marble can jump higher.  The Bubble Powerup allows a greater range of movement with both faster speed and floating you higher.";
		ultimateHint = "Pick up as many Time Travels as possible early on.";
		awesomeHint = "The Bubble Powerup is capable of giving you a speed boost. Use it in very short bursts, coupled with diagonal movement.";
		CustomRadarRule = $Radar::Flags::Gems | $Radar::Flags::EndPad | $Radar::Flags::Powerups;
		easterEgg = "0";
		gems = "6";
		interior0 = $usermods @ "/data/lbinteriors_pq/beginner/IntoTheDeep.dif";
		interiors = "1";
		maxScore = "6";
		trivia = "This level was completely redesigned from the ground up -- Matan did not think the original was a good enough level to teach about water and the Bubble Powerup. So he created a whole new level, making the previous one obsolete.";
};
}
