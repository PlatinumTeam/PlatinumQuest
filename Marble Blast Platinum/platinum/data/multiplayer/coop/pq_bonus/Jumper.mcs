		game = "PlatinumQuest";
		platinumTime = "90000";
		ultimateTime = "56000";
		generalHint = "Time your jumps carefully to soar high in the air, or alternatively get the Gems without too many problems.";
		ultimateHint = "The pink paddles are a quicker way of reaching the higher areas of the level than the fade platforms are.";
		eggHint = "You'll need a boost!";
		CustomRadarRule = $Radar::Flags::Gems | $Radar::Flags::TimeTravels | $Radar::Flags::EndPad;
		easterEgg = "1";
		gems = "10";
		interior0 = $usermods @ "/data/interiors_pq/bonus/JumperPQ.dif";
		interiors = "1";
		jumpImpulse = "14";
		maxScore = "10";
		trivia = "The level played a lot worse until Matan gave it some touch-ups and made it easier.";
};
}
