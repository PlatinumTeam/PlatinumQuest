//--- SERVER POSTMISSION SCRIPTS BEGIN ---
package IceShardGoToTarget {
	function IceShard::onCollision(%this, %ice, %marble, %unused1, %unused2, %material) {
		Parent::onCollision(%this, %ice, %marble, %unused1, %unused2, %material);

		if (%ice.gotoTarget) {
			if (%ice.TriggerOnce && %ice.resetTime == $Game::ResetTime)
				return;
			else
				%ice.resetTime = $Game::ResetTime;  //maybe port this system up to the RTGTT if it works well

			%i = 1;

			while (isObject(%ice.Object[%i]))
			{
				if (isObject(%ice.Path[%i]))
					%path = %ice.Path[%i];
				%ice.Object[%i].MoveOnPath(%path, %ice.InitialPosition[%i]);
				%i++;
			}

			%i = 1;
			while (isObject(%ice.pathedInterior[%i])) {
				%ice.pathedInterior[%i].setTargetPosition(%ice.targetTime[%i]);
				%i++;
			}

			%time = (%ice.persistTime ? %ice.persistTime : 5000);
			%marble.client.addBubbleLine(%ice.text, %ice.extended, %time);
		}
	}
};

activatePackage(IceShardGoToTarget);
//--- SERVER POSTMISSION SCRIPTS END ---