//DO NOT DELETE THIS LINE Polymorphism
datablock TriggerData(MultipleTGTT) { tickrateMS = 100; };

function MultipleTGTT::onAdd(%this, %trigger)
{
  if (%trigger.TargetTime $= "")
    %trigger.TargetTime = 0;
}

function MultipleTGTT::onEnterTrigger(%this, %trigger, %marble)
{
  if (%trigger.TargetTime < 0 || %trigger.TargetTime $= "") 
    return;
    
  plat1.setTargetPosition(%trigger.TargetTime);
  plat2.setTargetPosition(%trigger.TargetTime);
  plat3.setTargetPosition(%trigger.TargetTime);
  plat4.setTargetPosition(%trigger.TargetTime);
  plat5.setTargetPosition(%trigger.TargetTime);
  plat6.setTargetPosition(%trigger.TargetTime);
  plat7.setTargetPosition(%trigger.TargetTime);
  plat8.setTargetPosition(%trigger.TargetTime);
  plat9.setTargetPosition(%trigger.TargetTime);
}

