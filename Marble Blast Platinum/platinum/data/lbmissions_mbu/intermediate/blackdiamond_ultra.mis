datablock TriggerData(DoubleDiamondTrigger) {
	tickPeriodMS = 100;
};

function DoubleDiamondTrigger::onEnterTrigger(%this, %trigger, %obj) {
	if (%trigger.getName() $= "DoubleDiamondOne") {
		//First trigger - at the bottom

		//Enable the top trigger
		$Game::SpecialMode = true;
		devecho("This debug print officially declared 'a product of its time also yikes'");

		//Save their checkpoint in case they fall off
		%obj.client.checkpointDoubleDiamond = %obj.client.curCheckpointNum;

	} else if (%trigger.getName() $= "DoubleDiamondTwo") {
		//Second Trigger - at the top

		//Make sure they hit the bottom one first
		if ($Game::SpecialMode) {
			//Win here
			devecho("This one too. Final time is" SPC formatTime(PlayGui.elapsedTime));

			//End the game
			messageClient(%this, 'MsgRaceOver', '\c0Congratulations! You\'ve finished!');
			$Game::FinishClient = %obj.client;
			endGameSetup();
		}
	}
}


package DoubleDiamond {
	function GameConnection::respawnPlayer(%this, %pos) {
		Parent::respawnPlayer(%this, %pos);

		//If you respawn at all, you lose it (obviously)
		$Game::SpecialMode = false;

		//Also clear saved checkpoint
		%obj.client.checkpointDoubleDiamond = -1;
	}
	function GameConnection::respawnOnCheckpoint(%this) {
		Parent::respawnOnCheckpoint(%this);

		//If you hit the bottom after your last checkpoint, you don't get
		// to keep it. Try again.
		if (%this.curCheckpointNum <= %this.checkpointDoubleDiamond)
			$Game::SpecialMode = false;
	}
	function GameConnection::onEnterPad(%this) {
		//Wise guy
		if (%this.player.getPad() == $Game::EndPad) {
			$Game::SpecialMode = false;
		}
		//Do the normal finish-y thing
		Parent::onEnterPad(%this);
	}
};
activatePackage(DoubleDiamond);

